    <select id="selectPayerAccListByFundAccount" parameterType="java.util.HashMap" resultType="java.lang.String">
    select DISTINCT a.account_no from t_industry_account_info a JOIN t_industry b
    on a.industry_code=b.code and b.fund_account= #{fundAccount};
  </select>

