        {
            "name": "cardNumber",
            "type": "String",
            "mysql_type":"varchar(32)",
            "doc": "卡号"
        },
        {
             "name": "custAccountNo",
             "type": "String",
             "mysql_type":"varchar(32)",
             "doc": "客户账号"
