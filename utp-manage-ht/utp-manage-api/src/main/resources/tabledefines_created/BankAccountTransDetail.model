            "mysql_type":"varchar(8)",
            "doc": "交易账务日期，清算日期"
        },
        {
            "name": "natureDate",
            "type": "String",
            "mysql_type":"varchar(8)",
            "doc": "交易自然日期，交易日期"
        },
        {
            "name": "detailSerialNumber",
