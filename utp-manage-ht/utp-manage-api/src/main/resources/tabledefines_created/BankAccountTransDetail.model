            "doc": "借贷标志 0-借方 1-贷方"
        },
        {
            "name": "txnAmount",
            "type": "Long",
            "mysql_type":"bigint(20)",
            "doc": "交易金额（单位分)"
        },
        {
            "name": "accountBalance",
            "type": "Long",
            "mysql_type":"bigint(20)",
            "doc": "账户余额（单位分)"
        },
        {
           "name": "otherCustAccountNo",
