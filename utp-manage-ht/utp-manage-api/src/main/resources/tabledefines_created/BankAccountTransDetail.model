{
    "namespace": "com.scenetec.upf.operation",
    "module": "bankaccounttransdetail",
    "type": "table",
    "name": "BankAccountTransDetail",
    "mysqlname":"t_bank_account_trans_detail",
    "update":false,
    "doc": "未入账资金流水查询",
    "key":[],
    "index":[
        {
            "name":"t_bank_account_trans_detail_industry_code",
            "fields":["industryCode"],
            "unique":true
        },
        {
            "name":"t_bank_account_trans_detail_detail_serial_number",
            "fields":["detailSerialNumber"]
        },
        {
            "name":"t_bank_account_trans_detail_cust_account_noAndTxn_date",
            "fields":["custAccountNo","txnDate"]
        },
        {
            "name":"t_bank_account_trans_detail_accountNoAndTxnDateAndSeialNumber_UK",
            "fields":["custAccountNo","txnDate","detailSerialNumber"]
        }
    ],
    "conditions":[
        {
            "name":"industryCode",
            "operation":"=",
            "paramName":"industry_code"
        },
        {
            "name":"detailSerialNumber",
            "operation":"=",
            "paramName":"detail_serial_number"
        },
        {
            "name":"custAccountNo",
            "operation":"=",
            "paramName":"cust_account_no"
        },
        {
            "name":"txnDate",
            "operation":"=",
            "paramName":"txn_date"
        }
