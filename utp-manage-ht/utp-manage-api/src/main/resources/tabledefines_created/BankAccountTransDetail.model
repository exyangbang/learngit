           "mysql_type":"varchar(50)",
           "doc": "对方账号"
        },
        {
           "name": "otherCustName",
           "type": "String",
           "mysql_type":"varchar(50)",
           "doc": "对方客户名称"
        },
        {
           "name": "reversedFlag",
           "type": "String",
           "mysql_type":"varchar(10)",
           "doc": "0-正常 1-被冲账 2-冲账"
        },
        {
           "name": "remarkDetail",
           "type": "String",
           "mysql_type":"varchar(1024)",
           "doc": "备注信息"
        },
        {
           "name": "txnTime",
           "type": "String",
           "mysql_type":"varchar(6)",
           "doc": "交易时间HHmmss"
        },
        {
           "name": "sysReferenceNo",
           "type": "String",
           "mysql_type":"varchar(34)",
           "doc": "华通系统交易流水号"
        },
        {
           "name": "orgSysReferenceNo",
           "type": "String",
           "mysql_type":"varchar(34)",
           "doc": "华通原系统交易流水号"
        },
        {
           "name": "otherBankName",
           "type": "String",
           "mysql_type":"varchar(80)",
           "doc": "对方行名"
        },
        {
           "name": "otherRemark",
           "type": "String",
           "mysql_type":"varchar(80)",
           "doc": "对方银行备注"
